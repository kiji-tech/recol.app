{
    "cli": {
        "version": ">= 15.0.9",
        "appVersionSource": "remote"
    },
    "build": {
        "base": {
            "android": {
                "env": {
                    "PLATFORM": "android"
                }
            },
            "ios": {
                "env": {
                    "PLATFORM": "ios"
                }
            }
        },
        "development": {
            "extends": "base",
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_APP_ENV": "development"
            },
            "channel": "develop"
        },
        "development-simulator": {
            "extends": "base",
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "gradleCommand": ":app:assembleDebug"
            },
            "ios": {
                "simulator": true
            },
            "env": {
                "EXPO_PUBLIC_APP_ENV": "development"
            },
            "channel": "develop"
        },
        "preview": {
            "extends": "base",
            "channel": "develop",
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_APP_ENV": "development"
            }
        },
        "production": {
            "extends": "base",
            "autoIncrement": true,
            "channel": "main",
            "android": {
                "buildType": "app-bundle"
            },
            "env": {
                "EXPO_PUBLIC_APP_ENV": "production"
            }
        }
    },
    "submit": {
        "production": {
            "android": {
                "serviceAccountKeyPath": "./google-services.json",
                "track": "internal"
            }
        }
    }
}