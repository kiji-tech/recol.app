{
    "name": "Re:CoL",
    "version": "1.0.0",
    "main": "expo-router/entry",
    "scripts": {
        "check": "npx expo install --check",
        "start": "expo start --dev-client",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "stripe:dev": "stripe listen --forward-to localhost:54321/functions/v1/webhook/stripe",
        "prebuild": "expo prebuild",
        "postprebuild": "node fix-appdelegate.js",
        "postinstall": "node postinstall.mjs",
        "functions:dev": "npx supabase functions serve --env-file .env --no-verify-jwt",
        "functions:deploy": "npx supabase functions deploy",
        "generate:types": "export $(cat .env | grep -v \"#\" | xargs) && npx supabase gen types typescript --project-id ${EXPO_PUBLIC_SUPABASE_REFERENCE_ID} --schema public > src/libs/database.types.ts",
        "generate:types:local": "export $(cat .env | grep -v \"#\" | xargs) &&  npx supabase gen types --lang typescript --local --schema public > src/libs/database.types.ts",
        "test": "jest --coverage",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "ios:simulator": "open -a Simulator",
        "build:preview": "eas build --profile preview --platform android"
    },
    "dependencies": {
