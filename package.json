{
    "name": "Re:CoL",
    "main": "expo-router/entry",
    "scripts": {
        "check": "npx expo install --check",
        "start": "expo start --dev-client",
        "android": "expo run:android",
        "android:clear": "npx expo run:android --no-build-cache",
        "ios": "expo run:ios",
        "ios:install": "cd ios && rm -f Podfile.lock && pod repo update && pod install --repo-update",
        "ios:clear": "npx expo run:ios --no-build-cache",
        "ios:simulator": "open -a Simulator",
        "ios:simulator:remove": "xcrun simctl delete unavailable",
        "web": "expo start --web",
        "prebuild": "expo prebuild",
        "project:clear": "rm -rf node_modules && pnpm store prune && rm -f pnpm-lock.yaml && pnpm install",
        "postprebuild": "node fix-appdelegate.js",
        "postinstall": "node postinstall.mjs",
        "functions:dev": "npx supabase functions serve --env-file .env --no-verify-jwt",
        "functions:deploy": "npx supabase functions deploy",
        "generate:types": "export $(cat .env | grep -v \"#\" | xargs) && npx supabase gen types typescript --project-id ${EXPO_PUBLIC_SUPABASE_REFERENCE_ID} --schema public > src/libs/database.types.ts",
        "generate:types:local": "export $(cat .env | grep -v \"#\" | xargs) &&  npx supabase gen types --lang typescript --local --schema public > src/libs/database.types.ts && cp src/libs/database.types.ts supabase/functions/libs/database.types.ts",
        "test": "jest --coverage",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "build:preview": "eas build --profile preview --platform android",
        "build:production": "eas build --profile production --platform all",
        "build:production:android": "eas build --profile production --platform android",
        "build:production:ios": "eas build --profile production --platform ios",
        "submit:android": "eas submit --profile production --platform android",
        "submit:ios": "eas submit --profile production --platform ios",
        "open:ios": "npx uri-scheme open $link --ios"
    },
    "dependencies": {
        "@expo/metro-runtime": "~5.0.5",
        "@gorhom/bottom-sheet": "^5.1.6",
        "@react-native-async-storage/async-storage": "2.1.2",
        "@react-native-community/cli-server-api": "^18.0.0",
        "@react-native-community/datetimepicker": "8.4.1",
        "@react-native-google-signin/google-signin": "^15.0.0",
        "@react-native-picker/picker": "2.11.1",
        "@react-navigation/bottom-tabs": "^7.3.10",
        "@react-navigation/native": "^7.1.6",
        "@react-navigation/native-stack": "^7.3.10",
        "@supabase/supabase-js": "^2.50.3",
        "@tailwindcss/line-clamp": "^0.4.4",
        "@testing-library/react-native": "^13.2.2",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^30.0.0",
        "@types/react-native-maps": "^0.24.2",
        "axios": "^1.10.0",
        "chalk": "^5.4.1",
        "dayjs": "^1.11.13",
        "dom-to-image": "^2.6.0",
        "expo": "~53.0.23",
        "expo-apple-authentication": "^7.2.4",
        "expo-auth-session": "~6.2.1",
        "expo-build-properties": "~0.14.8",
        "expo-constants": "~17.1.7",
        "expo-dev-client": "~5.2.4",
        "expo-device": "^7.1.4",
        "expo-font": "~13.3.2",
        "expo-image": "~2.4.0",
        "expo-image-picker": "~16.1.4",
        "expo-linking": "~7.1.7",
        "expo-localization": "^17.0.7",
        "expo-location": "~18.1.6",
        "expo-media-library": "~17.1.7",
        "expo-notifications": "^0.31.4",
        "expo-router": "5.1.7",
        "expo-splash-screen": "~0.30.10",
        "expo-status-bar": "~2.2.3",
        "expo-system-ui": "~5.0.11",
        "expo-tracking-transparency": "~5.2.4",
        "expo-updates": "~0.28.17",
        "expo-web-browser": "~14.2.0",
        "nativewind": "4.1.23",
        "openai": "^5.8.2",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-i18next": "^15.5.3",
        "react-native": "0.79.6",
        "react-native-autolink": "^4.2.0",
        "react-native-dotenv": "^3.4.11",
        "react-native-elements": "^3.4.3",
        "react-native-gesture-handler": "~2.24.0",
        "react-native-google-mobile-ads": "15.4.0",
        "react-native-image-zoom-viewer": "^3.0.1",
