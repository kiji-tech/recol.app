    "expo-web-browser": "~14.0.2",
    "jest": "^29.7.0",
    "nativewind": "^4.1.23",
    "openai": "^4.82.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.4.0",
    "react-native": "0.76.9",
    "react-native-dotenv": "^3.4.11",
