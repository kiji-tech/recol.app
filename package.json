{
  "name": "voyz",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "node postinstall.js",
    "functions:dev": "npx supabase functions serve --env-file .env --no-verify-jwt",
    "functions:deploy": "npx supabase functions deploy",
    "generate:types": "export $(cat .env | grep -v \"#\" | xargs) && npx supabase gen types typescript --project-id ${EXPO_PUBLIC_SUPABASE_REFERENCE_ID} --schema public > src/libs/database.types.ts",
    "generate:types:local": "export $(cat .env | grep -v \"#\" | xargs) &&  npx supabase gen types --lang typescript --local --schema public > src/libs/database.types.ts",
    "test": "jest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "ios:simulator": "open -a Simulator"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@gorhom/bottom-sheet": "^5.0.5",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/cli-server-api": "^15.0.1-alpha.0",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@supabase/supabase-js": "^2.46.1",
    "@types/jest": "^29.5.14",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "dom-to-image": "^2.6.0",
    "eas-cli": "^12.6.2",
    "expo": "~51.0.38",
    "expo-build-properties": "~0.12.5",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.28",
    "expo-font": "~12.0.10",
    "expo-image-picker": "~15.0.7",
    "expo-linking": "~6.3.1",
    "expo-location": "~17.0.1",
    "expo-media-library": "~16.0.5",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "jest": "^29.7.0",
    "nativewind": "^4.1.21",
    "openai": "^4.69.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.75.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-maps": "^1.18.2",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "3.35.0",
    "react-native-svg": "^15.8.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-view-shot": "3.8.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@eslint/js": "^9.13.0",
    "@types/dom-to-image": "^2.6.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.4",
    "@types/react": "~18.3.12",
