    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.0",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.2.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "expo-doctor": "^1.12.5",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "prettier": "^3.4.2",
    "react-test-renderer": "18.2.0",
